    "supertest": "^3.1.0"
  },
  "dependencies": {
    "@koa/cors": "^2.2.2",
    "helmet": "^3.12.1",
    "ipaddr.js": "^1.8.0",
    "koa-helmet": "^4.0.0",
    "koa-log-lite": "^1.0.4"
  },
  "nyc": {
